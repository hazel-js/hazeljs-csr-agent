{
  "name": "hazeljs-csr-agent",
  "version": "1.0.0",
  "description": "Full-fledged Agent CSR example with @hazeljs packages - AI, RAG, Memory, Queue, WebSocket",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 0",
    "lint:fix": "eslint \"src/**/*.ts\" --fix --max-warnings 0"
  },
  "dependencies": {
    "@hazeljs/core": "file:../hazeljs/packages/core",
    "@hazeljs/ai": "file:../hazeljs/packages/ai",
    "@hazeljs/agent": "file:../hazeljs/packages/agent",
    "@hazeljs/rag": "file:../hazeljs/packages/rag",
    "@hazeljs/cache": "file:../hazeljs/packages/cache",
    "@hazeljs/config": "file:../hazeljs/packages/config",
    "@hazeljs/queue": "file:../hazeljs/packages/queue",
    "@hazeljs/websocket": "file:../hazeljs/packages/websocket",
    "@hazeljs/swagger": "file:../hazeljs/packages/swagger",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "ioredis": "^5.4.1",
    "openai": "^4.0.0",
    "reflect-metadata": "^0.2.1",
    "swagger-ui-express": "^5.0.0"
  },
  "optionalDependencies": {
    "@pinecone-database/pinecone": "^3.0.0",
    "@qdrant/js-client-rest": "^1.9.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
